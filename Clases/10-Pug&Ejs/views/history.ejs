<h3>Historial</h3>

<% if (personas.length > 0){%>
    <div>
        <table class="table table-dark">
            <tr class="text-warning">
                <th>Nombre</th>
                <th>Apellido</th>
                <th>Edad</th>
            </tr>
            <% personas.forEach(persona=>{%>
                <tr>
                    <td><%= persona.nombre%></td>
                    <td><%= persona.apellido%></td>
                    <td><%= persona.edad%></td>
                </tr>
            <%}) %>
        </table>
    </div>
<%}else{%>
    <h3 class="alert alert-danger">No se encuentran datos.</h3>
<%}%>